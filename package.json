    "@types/crypto-js": "^4.2.2",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.10.0",
    "cypress": "^14.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^14.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fixed-jsdom": "^0.0.9",
    "lerna": "^8.2.2",
    "lint-staged": "^15.5.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "serve": "^14.2.4",
    "source-map": "^0.6.1",
    "source-map-support": "^0.5.13",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.8.3",
    "web-vitals": "^4.2.4",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  },
  "optionalDependencies": {
    "sass-embedded": "^1.89.0"
  },
  "scripts": {
    "start": "npx webpack-dev-server --progress --config ./webpack.config.js --mode development --host 0.0.0.0 --port 3000 --env development",
    "build": "webpack --config webpack.config.js --mode production --env production",
    "build:analyze": "webpack --config webpack.config.js --mode production --env production --env analyze",
    "preview": "npm run build && echo 'El frontend est√° listo en /dist para ser servido por NGINX.'",
    "check:coverage": "node checkCoverage.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --clearCache && jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:prod": "NODE_ENV=production cypress run",
    "test:e2e:prod:open": "NODE_ENV=production cypress open",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\"",
    "prepare": "husky install",
    "commit": "cz",
    "update:snapshots": "jest -u",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:start": "lerna run --scope=@acamae/frontend start",
    "lerna:build": "lerna run build",
    "lerna:test": "lerna run test",
    "lerna:lint": "lerna run lint",
    "lerna:publish": "lerna publish from-package --yes",
    "docker:build:nocache": "docker compose -f docker/docker-compose.yml build --no-cache",
    "docker:up": "docker compose -f docker/docker-compose.yml up",
    "docker:up:build": "docker compose -f docker/docker-compose.yml up --build",
    "docker:down": "docker compose -f docker/docker-compose.yml down -v --remove-orphans",
    "docker:reset": "docker compose -f docker/docker-compose.yml down -v --rmi all --remove-orphans"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
