{
  "packages": ["."],
  "version": "0.10.3",
  "npmClient": "npm",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "changelogPreset": "conventionalcommits",
      "message": "chore(release): %s [skip ci]",
      "noPrivate": true,
      "createRelease": "github",
      "changelog": {
        "preset": "conventionalcommits",
        "releaseCount": 0,
        "outputUnreleased": true
      }
    }
  }
}
